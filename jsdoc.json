{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": [
        "./src",
        "./README.md"
      ],
      "includePattern": ".+\\.(js|md)$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "docs/api",
      "recurse": true,
      "verbose": true,
      "private": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}